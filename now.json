{
  "name": "joshuawebsite",
  "version": 2,
  "builds": [
    {
      "src": "nuxt/package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "nuxt/build"
      }
    }
  ],
  "build": {
    "env": {
      "SEOCONFIG": "@seoconfig",
      "AUTHCONFIG": "@authconfig",
      "APIURL": "@apiurl",
      "AMPURL": "@ampurl"
    }
  },
  "routes": [
    {
      "src": "/blog/(.*)",
      "dest": "/blog"
    },
    {
      "src": "/post/(.*)",
      "dest": "/post"
    }
  ]
}
